                  <desc>Foll. <locus>58—67</locus>, which are in a more current hand of the <date
                      from="0800" to="1000">ixth or xth cent</date>.</desc>
                </handNote>
                <handNote xml:id="p1handNote3" scope="minor" script="syr" medium="unknown">
                  <desc><ref target="#p1addition1">See additions.</ref></desc>
                </handNote>
                <handNote xml:id="p1handNote4" scope="minor" script="syr" medium="unknown">

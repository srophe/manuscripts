            <msContents>
              <summary/>
              <textLang mainLang="syr">Syriac</textLang>
              <msItem n="1" xml:id="p1a1" defective="true">
                <locus from="1b" to="107">Foll. 1b-107</locus>
                <title type="supplied" ref="http://syriaca.org/work/203">The first two Gospels, according to the Peshitta version</title>
                <msItem n="2" xml:id="p1b1" defective="true">

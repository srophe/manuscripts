            <msContents>
              <summary/>
              <textLang mainLang="syr">Syriac</textLang>
              <msItem n="1" xml:id="p3a1">
                <locus from="53" to="101">Foll. 53-101</locus>
                <title type="supplied" ref="http://syriaca.org/work/7">The book of Judges, according to the Peshitta version</title>
                <rubric xml:lang="syr">ܣܦܪ ܕܝ̈ܢܐ ܕܒܢ̈ܝ ܐܝܣܪܐܝܠ ܕܡܬܩܪܐ ܥܒܪܐܝܬ ܫ̈ܦܛܐ<locus from="52" to="52"/></rubric>
              </msItem>
            </msContents>
            <physDesc>
              <objectDesc form="codex">
                <supportDesc material="perg">
                  <support><material>Vellum</material></support>
                  <extent>
                    <measure type="composition" unit="leaf" quantity="49">49 ff.</measure>
                    <dimensions type="leaf" unit="in">
                      <height>
                        <measure type="height" quantity="9.125" unit="in">9 1/8</measure>
                      </height>
                      <width>
                        <measure type="width" quantity="5.75" unit="in">5 3/4</measure>
                      </width>
                    </dimensions>
                  </extent>
                  <foliation/>
                  <collation>
                    <p>The quires are now signed with letters from <foreign xml:lang="syr">ܚ</foreign> to <foreign xml:lang="syr">ܝܒ</foreign>.</p>
                  </collation>
                  <condition/>
                </supportDesc>
                <layoutDesc>
                  <layout columns="1" writtenLines="23 26">
                    <p>Each page has from 23 to 26 lines.</p>
                  </layout>

              <handDesc hands="8">
                <handNote xml:id="p2handNote1" scope="major" script="syr-Syre" medium="unknown">
                  <desc>The writing is a good, regular Estrangela of the <origDate from="0700" to="0800">viiith cent.</origDate>. <persName ref="">Simeon</persName></desc>
                </handNote>
                <handNote xml:id="p2handNote2" scope="minor" script="syr" medium="unknown">
                  <desc>See <ref target="#p21addition1">additions</ref>.</desc>
                </handNote>

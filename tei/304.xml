                <handNote xml:id="handNote1" scope="major" script="syr-Syre" medium="unknown">
                  <desc>This manuscript is written in a fine, regular Estrangela, and dated A.D. <origDate when="0519" calendar="Gregorian">519</origDate>. <ref target="#addition1">See additions.</ref>
                  </desc>
                </handNote>
                <handNote xml:id="handNote2" scope="minor" script="syr" medium="unknown">
                  <desc><ref target="#addition2">See additions.</ref></desc>
                </handNote>

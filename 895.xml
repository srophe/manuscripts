                <handNote xml:id="p2handNote1" scope="sole" script="syr" medium="unknown">
                  <desc>Two vellum leaves written in a neat, regular hand, and dated <origDate calendar="Greek" when="1150">A. Gr. 1150</origDate>, <origDate calendar="Gregorian" when="0839">A.D. 839</origDate></desc>
                </handNote>

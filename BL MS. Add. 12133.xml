                            <handDesc>
                                <handNote xml:id="handnotep2a1" scope="major" script="Estrangelo" medium="unknown">
                                    <p>The writing is a fine, regular Estrangela of the <origDate notBefore="0700" notAfter="0800">viiith century</origDate>.</p>
                                <!-- VOWELS  -->
                                    <locus from="109" to="169">ff. 109-169</locus>
                                    <listPerson>
                                        <person role="scribe" xml:id="Paul_Tella" corresp="">
                                            <persName xml:lang="eng">Paul of Tellā</persName>
                                            <persName xml:lang="syr"/>
                                        </person>

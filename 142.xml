            <msContents>
              <summary/>
              <textLang mainLang="syr">Syriac</textLang>
               <msItem n="1" xml:id="a1" defective="true">
                  <locus from="1a" to="130">Foll. 1a-130</locus>
                  <title type="supplied" ref="http://syriaca.org/work/71">The four Gospels, according to the Peshitta version</title>
                  <msItem n="2" xml:id="b1" defective="true">
                     <locus from="1a" to="36">Foll. 1a-36</locus>
                     <title type="supplied" ref="http://syriaca.org/work/72">Matthew</title>
                     <note>Missing portions: ch. i. 1—v. 24, viii. 19—xviii. 25, and xxi. 2—16.</note>
                  </msItem>
                  <msItem n="3" xml:id="b2">
                     <locus from="36b" to="80">Foll. 36b-80</locus>
                     <title type="supplied" ref="http://syriaca.org/work/73">Mark</title>
                  </msItem>
                  <msItem n="4" xml:id="b3" defective="true">
                     <locus from="80a" to="111">Foll. 80a-111</locus>
                     <title type="supplied" ref="http://syriaca.org/work/74">Luke</title>
                     <note>Missing portions: ch. i. 56—x. 35, xi. 25—39, xvii. 29—xviii. 9, and xxii. 35 to the end.</note>
                  </msItem>
                  <msItem n="5" xml:id="b4" defective="true">
                     <locus from="111a" to="130">Foll. 111a-130</locus>
                     <title type="supplied" ref="http://syriaca.org/work/75">John</title>
                     <note>Missing portions: ch. i. 1—37, v. 37—xvi. 10, and xx. 1 to the end.</note>
                  </msItem>
                  <note>The lessons are marked throughout, usually on the margins, by the hand of the scribe.</note>
               </msItem>

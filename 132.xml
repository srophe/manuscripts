              <handDesc hands="4">
                <handNote xml:id="p1handNote1" scope="major" script="syr-Syre" medium="unknown">
                   <desc>This manuscript is written in a beautiful, Edessene Estrangela, apparently of the <origDate notBefore="0400" notAfter="0500">5th century</origDate>.</desc>
                </handNote>
                 <handNote xml:id="p1handNote2" scope="minor" script="syr" medium="unknown">
                    <desc>Perhaps of the <date notBefore="0900" notAfter="1000">10th century</date>. <ref target="#p1addition1">See additions.</ref></desc>
                 </handNote>
                 <handNote xml:id="p1handNote3" scope="minor" script="syr" medium="unknown">
                    <desc><ref target="#p1addition2">See additions.</ref></desc>
                 </handNote>
                 <handNote xml:id="p1handNote4" scope="minor" script="syr" medium="unknown">

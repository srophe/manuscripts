                            <handDesc>
                                <handNote xml:id="p2handnote1" scope="major" script="syr-Syre" medium="unknown">
                                    <p>The writing is a fine, regular Estrangela of the <origDate notBefore="0700" notAfter="0800">viiith century</origDate>.</p>
                       <!-- VOWELS  -->
                                    <locus from="109" to="169"/>
                                    <listPerson>
                                        <person role="scribe" xml:id="Paul_Tella" corresp="http://syriaca.org/person/82">

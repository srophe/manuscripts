            <msContents>
              <summary/>
              <textLang mainLang="syr">Syriac</textLang>
              <msItem n="1" xml:id="a1">
                <locus from="1a" to="197a">Foll. 1a-197a</locus>
                <title type="supplied" ref="http://syriaca.org/work/180">The Ammonian sections and Eusebian canons</title>
                <note>They are marked in the ordinary way.</note>
              </msItem>
              <msItem n="2" xml:id="a2">
                <locus from="1a" to="197a">Foll. 1a-197a</locus>
                <title type="supplied" ref="http://syriaca.org/work/181">Gospel harmony</title>
                <note>They are marked at the foot of each page.</note>
              </msItem>
               <msItem n="3" xml:id="a3" defective="true">
                  <locus from="1a" to="197a">Foll. 1a-197a</locus>
                  <title ref="http://syriaca.org/work/71" type="supplied">The four Gospels, according to the Peshitta version</title>
                  <msItem n="4" xml:id="b1" defective="true">
                     <locus from="1a" to="50b">Foll. 1a-50b</locus>
                    <title ref="http://syriaca.org/work/72" type="supplied">The Gospel of S. Matthew, commencing with ch. v. 46, according to the Peshitta version</title>
                     <finalRubric xml:lang="syr"><locus from="50b" to="50b"/>ܫܠܡܬ ܟܪܘܙܘܬܐ ܕܡܬܝ ܫܠܝܚܐ ܕܡܠܠ ܥܒܪܐܝܬ ܒܦܠܝܣܛܝܢܐ.</finalRubric>
                  </msItem>
                  <msItem n="5" xml:id="b2">
                     <locus from="50b" to="86b">Foll. 50b-86b</locus>
                    <title ref="http://syriaca.org/work/73" type="supplied">The Gospel of S. Mark, according to the Peshitta version</title>
                     <rubric xml:lang="syr"><locus from="50b" to="50b"/>ܐܘܢܓܠܝܘܢ ܩܕܝܫܐ ܟܪܘܙܘܬܐ ܕܡܪܩܘܣ</rubric>
                     <finalRubric xml:lang="syr"><locus from="86b" to="86b"/>ܫܠܡ ܐܘܢܓܠܝܘܢ ܩܕܝܫܐ ܟܪܘܙܘܬܐ ܕܡܪܩܘܣ ܕܡܠܠ ܪܗܘܡܐܝܬ ܒܪܗܘܡܐ.</finalRubric>
                  </msItem>
                  <msItem n="6" xml:id="b3">
                     <locus from="87b" to="150a">Foll. 87b-150a</locus>
                    <title ref="http://syriaca.org/work/74" type="supplied">The Gospel of Luke, according to the Peshitta version</title>
                     <rubric xml:lang="syr"><locus from="87b" to="87b"/>ܐܘܢܓܠܝܘܢ ܩܕܝܫܐ ܟܪܘܙܘܬܐ ܕܠܘܩܐ ܡܣܒܪܢܐ</rubric>
                     <finalRubric xml:lang="syr"><locus from="150a" to="150a"/>ܫܠܡ ܐܘܢܓܠܝܘܢ ܩܕܝܫܐ ܟܪܘܙܘܬܐ ܕܠܘܩܐ ܡܣܒܪܢܐ ܕܡܠܠ ܝܘܢܐܝܬ ܒܐܠܟܣܢܕܪܝܐ ܪܒܬܐ</finalRubric>
                  </msItem>
                  <msItem n="7" xml:id="b4">
                     <locus from="150a" to="197a">Foll. 150a-197a</locus>
                    <title ref="http://syriaca.org/work/75" type="supplied">The Gospel of John, according to the Peshitta version</title>
                     <rubric xml:lang="syr"><locus from="150a" to="150a"/>ܐܘܢܓܠܝܘܢ ܩܕܝܫܐ ܟܪܘܙܘܬܐ ܕܝܘܚܢܢ</rubric>
                     <finalRubric xml:lang="syr"><locus from="197a" to="197a"/>ܫܠܡ ܐܘܢܓܠܝܘܢ ܩܕܝܫܐ ܟܪܘܙܘܬܐ ܕܝܘܚܢܢ ܫܠܝܚܐ ܕܡܠܠ ܝܘܢܐܝܬ ܒܐܦܣܣ.</finalRubric>
                  </msItem>
                 <finalRubric xml:lang="syr"> ܫܠܡ [ܠܡܟܬܒ] ܒ[ܟܬܒܐ ܗܢܐ] ܛܛ[ܪܐܘܢܓܠܝܘܢ] ܩܕܝܫܐ [ܡܬܝ] ܡܪܩܘܣ ܠܘ[ܩܐ] ܝܘܚܢܢ ܕܐܟܪܙܘ[ܗܝ] ܘܣܒܪܘܗܝ ܒܟܠ ܐܬܪ ܘܡܕ[ܝܢܐ.]
                   <locus from="197a" to="197a"/></finalRubric>
                 <finalRubric type="doxology">The usual doxology follows.<locus from="197a" to="197a"/></finalRubric> 
                 <note>Some lessons are rubricated in the text.</note>
               </msItem>

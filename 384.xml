              <handDesc hands="">
                <handNote xml:id="p2handNote1" scope="major" script="syr" medium="unknown">
                  <desc>This manuscript is written in a good, clear hand of the <origDate notBefore="0800" notAfter="0900">ixth cent.</origDate> The letter <quote xml:lang="syr">ܘ</quote> when initial, is often disproportionately large in comparison with the other letters. <ref target="p2addition3">See additions.</ref></desc>
                </handNote>
                <handNote xml:id="p2handNote2" scope="minor" script="syr" medium="unknown">
                  <desc><ref target="#p2addition1">See additions</ref></desc> 

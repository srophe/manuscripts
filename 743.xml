                  <extent>
                    <measure type="composition" unit="leaf" quantity="61">61 ff.</measure>
                    <dimensions type="leaf" unit="in">
                      <height>
                        <measure type="height" quantity="10.625" unit="in">10 5/8 in.</measure>

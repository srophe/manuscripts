                      <persName>Timachus (Timarchus ?)</persName>
                      <note><locus from="120a">Fol. 120 a</locus></note>
                    </person>
                    <person>
                      <persName><foreign xml:lang="syr">ܬܐܩܪܝܕܣ</foreign></persName> 
                      <persName>Theocritus</persName>
                     <note><locus from="120a">Fol. 120 a</locus></note>
                    </person>

            <physDesc>
              <objectDesc form="codex">
                <supportDesc material="mixed">
                  <support>
                    <material>Vellum</material>
                    <material>Paper</material>
                  </support>
                  <extent>
                    <measure type="composition" unit="leaf" quantity="110">110 ff.</measure>
                    <dimensions type="leaf" unit="in">
                      <height>
                        <measure type="height" quantity="11" unit="in">11</measure>
